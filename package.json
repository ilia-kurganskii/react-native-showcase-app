{
  "name": "nucleusuipatternexamples",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "typescript": "tsc",
    "prepare": "husky install",
    "detox:build:ios": "detox build -c ios",
    "detox:build:ios-ci": "detox build -c ios.ci",
    "detox:build:ios-tdd": "detox build -c ios.tdd",
    "detox:build:android": "detox build -c android",
    "detox:build:android-tdd": "detox build -c android.tdd",
    "detox:test:ios": "detox test -c ios",
    "detox:test:ios-ci": "detox test -c ios.ci",
    "detox:test:android": "detox test -c android",
    "detox:test:ios-tdd": "detox test -c ios.tdd",
    "detox:test:android-tdd": "detox test -c android.tdd",
    "detox:cache": "detox build-framework-cache"
  },
  "dependencies": {
    "@react-native-firebase/app": "^14.11.1",
    "@react-native-firebase/auth": "^14.11.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.2",
    "mobx": "^6.6.0",
    "mobx-react-lite": "^3.4.0",
    "react": "17.0.2",
    "react-native": "0.68.2",
    "react-native-nucleus-ui": "^0.1.4",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-screens": "^3.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-decorators": "^7.18.2",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^3.0.0",
    "@types/detox": "^18.1.0",
    "@types/jest": "^27.5.1",
    "@types/react-native": "^0.67.3",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-jest": "^26.6.3",
    "detox": "^19.7.1",
    "eslint": "^7.32.0",
    "husky": ">=6",
    "jest": "^28.0.2",
    "jest-circus": "^28.1.0",
    "lint-staged": ">=10",
    "metro-react-native-babel-preset": "^0.67.0",
    "react-test-renderer": "17.0.2",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "testRegex": "\\.spec.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  }
}
