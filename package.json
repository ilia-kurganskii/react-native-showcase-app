{
  "name": "nucleusuipatternexamples",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "translate:extract": "i18next-scanner",
    "translate:check": "i18next-scanner && ! grep -lrnw ./src/features/i18n/resources -e '__STRING_NOT_TRANSLATED__'",
    "typescript": "tsc",
    "prepare": "husky install",
    "detox:build:ios": "detox build -c ios",
    "detox:build:ios-ci": "detox build -c ios.ci",
    "detox:build:ios-tdd": "detox build -c ios.tdd",
    "detox:build:android": "detox build -c android",
    "detox:build:android-tdd": "detox build -c android.tdd",
    "detox:test:ios": "detox test -c ios",
    "detox:test:ios-ci": "detox test -c ios.ci",
    "detox:test:android": "detox test -c android",
    "detox:test:ios-tdd": "detox test -c ios.tdd",
    "detox:test:android-tdd": "detox test -c android.tdd",
    "detox:cache": "detox build-framework-cache"
  },
  "dependencies": {
    "@react-native-firebase/app": "14.11.1",
    "@react-native-firebase/auth": "14.11.1",
    "@react-native-firebase/crashlytics": "14.11.1",
    "@react-navigation/bottom-tabs": "6.3.2",
    "@react-navigation/native": "6.0.11",
    "@react-navigation/native-stack": "6.7.0",
    "axios": "0.27.2",
    "formik": "2.2.9",
    "i18next": "21.8.14",
    "lottie-react-native": "5.1.3",
    "mobx": "6.6.1",
    "mobx-react-lite": "3.4.0",
    "react": "18.2.0",
    "react-i18next": "11.18.1",
    "react-native": "0.69.1",
    "react-native-file-logger": "0.3.0",
    "react-native-nucleus-ui": "0.3.0",
    "react-native-safe-area-context": "4.3.1",
    "react-native-screens": "3.15.0",
    "react-native-splash-screen": "3.3.0",
    "react-native-svg": "12.4.0",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@babel/core": "7.18.9",
    "@babel/plugin-proposal-decorators": "7.18.9",
    "@babel/runtime": "7.18.9",
    "@react-native-community/eslint-config": "3.0.3",
    "@types/detox": "18.1.0",
    "@types/jest": "28.1.6",
    "@types/react-native": "0.69.3",
    "@types/react-test-renderer": "18.0.0",
    "@typescript-eslint/eslint-plugin": "5.31.0",
    "@typescript-eslint/parser": "5.31.0",
    "babel-jest": "28.1.3",
    "babel-plugin-module-resolver": "4.1.0",
    "detox": "19.8.4",
    "dotenv": "16.0.1",
    "eslint": "8.20.0",
    "eslint-plugin-i18next": "6.0.0-4",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.7.0",
    "husky": "8.0.1",
    "i18next-scanner": "4.0.0",
    "jest": "28.1.3",
    "jest-circus": "28.1.3",
    "lint-staged": "13.0.3",
    "metro-react-native-babel-preset": "0.72.0",
    "prettier": "2.7.1",
    "react-native-svg-transformer": "1.0.0",
    "react-test-renderer": "18.2.0",
    "ts-jest": "28.0.7",
    "typescript": "4.7.4"
  },
  "resolutions": {
    "@types/react": "18"
  },
  "jest": {
    "preset": "react-native",
    "testRegex": "\\.spec.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  }
}
