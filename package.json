{
  "name": "nucleusuipatternexamples",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "translate:extract": "i18next-scanner",
    "translate:check": "i18next-scanner && ! grep -lrnw ./src/features/i18n/resources -e '__STRING_NOT_TRANSLATED__'",
    "typescript": "tsc",
    "prepare": "husky install",
    "detox:build:ios": "detox build -c ios",
    "detox:build:ios-ci": "detox build -c ios.ci",
    "detox:build:ios-tdd": "detox build -c ios.tdd",
    "detox:build:android": "detox build -c android",
    "detox:build:android-tdd": "detox build -c android.tdd",
    "detox:test:ios": "detox test -c ios",
    "detox:test:ios-ci": "detox test -c ios.ci",
    "detox:test:android": "detox test -c android",
    "detox:test:ios-tdd": "detox test -c ios.tdd",
    "detox:test:android-tdd": "detox test -c android.tdd",
    "detox:cache": "detox build-framework-cache",
    "generate": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "15.16.2",
    "@contentful/rich-text-types": "15.15.1",
    "@react-native-firebase/app": "14.11.1",
    "@react-native-firebase/auth": "14.11.1",
    "@react-native-firebase/crashlytics": "14.11.1",
    "@react-navigation/bottom-tabs": "6.3.3",
    "@react-navigation/native": "6.0.12",
    "@react-navigation/native-stack": "6.8.0",
    "@reduxjs/toolkit": "1.8.5",
    "axios": "0.27.2",
    "formik": "2.2.9",
    "graphql": "16.6.0",
    "i18next": "21.10.0",
    "inversify": "6.0.1",
    "lottie-react-native": "5.1.4",
    "react": "18.2.0",
    "react-i18next": "11.18.6",
    "react-native": "0.70.6",
    "react-native-file-logger": "0.3.0",
    "react-native-nucleus-ui": "0.3.0",
    "react-native-safe-area-context": "4.3.3",
    "react-native-screens": "3.17.0",
    "react-native-splash-screen": "3.3.0",
    "react-native-svg": "13.1.0",
    "react-redux": "8.0.5",
    "redux-saga": "^1.2.1",
    "reflect-metadata": "0.1.13",
    "urql": "3.0.3",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@babel/core": "7.18.13",
    "@babel/plugin-proposal-decorators": "7.18.10",
    "@babel/runtime": "7.18.9",
    "@graphql-codegen/cli": "2.12.0",
    "@graphql-codegen/near-operation-file-preset": "2.4.0",
    "@graphql-codegen/schema-ast": "2.5.1",
    "@graphql-codegen/typescript": "2.7.2",
    "@graphql-codegen/typescript-document-nodes": "2.3.2",
    "@graphql-codegen/typescript-operations": "2.5.2",
    "@react-native-community/eslint-config": "3.1.0",
    "@types/detox": "18.1.0",
    "@types/jest": "29.0.0",
    "@types/react-native": "0.70.6",
    "@types/react-test-renderer": "18.0.0",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "babel-jest": "29.0.3",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "detox": "20.1.0",
    "dotenv": "16.0.2",
    "eslint": "8.22.0",
    "eslint-plugin-i18next": "6.0.0-4",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.0.1",
    "husky": "8.0.1",
    "i18next-scanner": "4.0.0",
    "jest": "29.0.2",
    "jest-circus": "29.0.2",
    "lint-staged": "13.0.3",
    "metro-react-native-babel-preset": "0.72.3",
    "prettier": "2.7.1",
    "react-native-svg-transformer": "1.0.0",
    "react-test-renderer": "18.2.0",
    "ts-jest": "29.0.3",
    "typescript": "4.7.4"
  },
  "resolutions": {
    "@types/react": "18"
  },
  "jest": {
    "preset": "react-native",
    "testRegex": "\\.spec.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  }
}
